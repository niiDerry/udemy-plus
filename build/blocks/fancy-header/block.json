{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "udemy-plus/fancy-header",
  "title": "Fancy Header",
  "category": "text",
  "description": "Adds a header with a hover effect.",
  "keywords": [
    "header",
    "underlined",
    "hover"
  ],
  "version": "1",
  "textdomain": "udemy-plus",
  "editorScript": "file:./index.js",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    },
    "underlined_color": {
      "type": "string",
      "default": "#f87171"
    }
  },
  "style": "file:./index.css"
}